{
  "name": "prometheus-mssql-exporter",
  "version": "1.0.0",
  "main": "index.js",
  "private": false,
  "keywords": [
    "prometheus",
    "mssql",
    "exporter",
    "metrics"
  ],
  "author": {
    "name": "Pierre Awaragi",
    "email": "pierre@awaragi.com"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/awaragi/prometheus-mssql-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/awaragi/prometheus-mssql-exporter/issues"
  },
  "scripts": {
    "build": "docker build . -t awaragi/prometheus-mssql-exporter",
    "test:serve": "DEBUG=app,db,metrics SERVER=localhost USERNAME=SA PASSWORD=qkD4x3yy node ./index.js",
    "test:serve:verbose": "DEBUG=app,db,metrics,metrics SERVER=localhost USERNAME=SA PASSWORD=qkD4x3yy node ./index.js",
    "test:mssql:2019": "docker run --rm -e ACCEPT_EULA=Y -e SA_PASSWORD=qkD4x3yy -p 1433:1433 --name mssql mcr.microsoft.com/mssql/server:2019-latest",
    "test:mssql:2017": "docker run --rm -e ACCEPT_EULA=Y -e SA_PASSWORD=qkD4x3yy -p 1433:1433 --name mssql mcr.microsoft.com/mssql/server:2017-latest",
    "test:fetch": "curl http://localhost:4000/metrics",
    "test": "jest test"
  },
  "dependencies": {
    "debug": "2.6.8",
    "express": "4.15.2",
    "prom-client": "9.1.1",
    "tedious": "2.0.0"
  },
  "devDependencies": {
    "jest": "27.5.1",
    "nodemon": "1.11.0",
    "prettier": "2.6.0",
    "superagent": "^7.1.1"
  }
}
